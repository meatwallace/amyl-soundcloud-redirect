{
  "name": "amyl-soundcloud-redirect",
  "alias": ["amyl.stream"],
  "scope": "meatwallace",
  "regions": ["all"],
  "public": false,
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://soundcloud.com/amylnitrate/$1"
      }
    }
  ]
}

